    "A -= B\n",
    "print(\"A после удаления общих элементов:\", A)\n",
    "\n",
    "# b)\n",
    "user1 = {'laptop', 'mouse', 'keyboard'}\n",
    "user2 = {'mouse', 'headphones', 'monitor'}\n",
    "user3 = {'keyboard', 'webcam', 'mouse'}\n",
    "all_products = user1 | user2 | user3\n",
    "print(\"Все купленные товары:\", all_products)"
