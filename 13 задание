   "id": "26f1d2be-c43b-420d-98e9-5f213e4176b7",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Введите текст:  абракадабра hello world\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Гласных: 3, Согласных: 9\n",
      "Больше согласных\n",
      "Только во второй день: {'grape', 'pear'}\n"
     ]
    }
   ],
   "source": [
    "#11\n",
    "# a)\n",
    "text = input(\"Введите текст: \").lower()\n",
    "vowels = set('аеёиоуыэюяaeiou')\n",
    "consonants = set('бвгджзйклмнпрстфхцчшщbcdfghjklmnpqrstvwxyz')\n",
    "text_letters = set(text) & (vowels | consonants)\n",
    "vowel_count = len(text_letters & vowels)\n",
    "consonant_count = len(text_letters & consonants)\n",
    "print(f\"Гласных: {vowel_count}, Согласных: {consonant_count}\")\n",
    "print(\"Больше гласных\" if vowel_count > consonant_count else \"Больше согласных\")\n",
    "\n",
    "# b)\n",
    "day1_orders = {'apple', 'banana', 'orange'}\n",
    "day2_orders = {'banana', 'grape', 'pear'}\n",
    "day3_orders = {'apple', 'kiwi', 'mango'}\n",
    "specific_day = day2_orders - day1_orders - day3_orders\n",
    "print(\"Только во второй день:\", specific_day)"
   ]
  },
