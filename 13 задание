    "# a)\n",
    "s1 = input(\"Первая строка: \")\n",
    "s2 = input(\"Вторая строка: \")\n",
    "target = input(\"Целевая строка: \")\n",
    "available_chars = set(s1 + s2)\n",
    "print(\"Можно составить:\", set(target).issubset(available_chars))\n",
    "\n",
    "# b)\n",
    "emp1 = {'python', 'java', 'sql'}\n",
    "emp2 = {'python', 'javascript', 'html'}\n",
    "emp3 = {'java', 'c++', 'css'}\n",
    "unique_skills = (emp1 ^ emp2 ^ emp3) - (emp1 & emp2) - (emp1 & emp3) - (emp2 & emp3)\n",
    "print(\"Уникальные навыки:\", unique_skills)"
