    "#9\n",
    "# a)\n",
    "text = input(\"Введите текст: \").lower()\n",
    "vowels = set('аеёиоуыэюя')\n",
    "found_vowels = sorted(set(text) & vowels)\n",
    "print(\"Гласные в тексте:\", found_vowels)\n",
    "\n",
    "# b)\n",
    "products = {\n",
    "    'prod1': {'electronics', 'phones'},\n",
    "    'prod2': {'electronics', 'computers'}, \n",
    "    'prod3': {'books', 'education'},\n",
    "    'prod4': {'books'}\n",
    "}\n",
    "all_categories = set()\n",
    "multiple_products = set()\n",
    "for categories in products.values():\n",
    "    for cat in categories:\n",
    "        if cat in all_categories:\n",
    "            multiple_products.add(cat)\n",
    "        all_categories.add(cat)\n",
    "single_product_categories = all_categories - multiple_products\n",
    "print(\"Категории с одним продуктом:\", single_product_categories)"
   ]
  },
