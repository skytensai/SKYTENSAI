    "#1\n",
    "# a)\n",
    "s = input(\"Введите последовательность: \")\n",
    "digits = set(c for c in s if c.isdigit())\n",
    "operators = set(c for c in s if c in '+-*/')\n",
    "letters_AF = set(c for c in s if 'A' <= c <= 'F')\n",
    "letters_XZ = set(c for c in s if 'X' <= c <= 'Z')\n",
    "print(\"Цифры:\", digits)\n",
    "print(\"Операторы:\", operators)\n",
    "print(\"Буквы A-F:\", letters_AF)\n",
    "print(\"Буквы X-Z:\", letters_XZ)\n",
    "\n",
    "# b)\n",
    "all_codes = set(range(1, 21))\n",
    "given_codes = set(map(int, input(\"Введите коды товаров через пробел: \").split()))\n",
    "missing = all_codes - given_codes\n",
    "print(\"Отсутствуют:\", missing)"
   ]
