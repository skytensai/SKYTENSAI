    "#12\n",
    "s = input()\n",
    "result = ''\n",
    "for char in s:\n",
    "    if char.isalpha():\n",
    "        if char.islower():\n",
    "            result += chr((ord(char) - ord('a') + 1) % 26 + ord('a'))\n",
    "        else:\n",
    "            result += chr((ord(char) - ord('A') + 1) % 26 + ord('A'))\n",
    "    else:\n",
    "        result += char\n",
    "print(result)"
   ]
