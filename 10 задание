    "#14\n",
    "s = input().split()\n",
    "result = []\n",
    "for word in s:\n",
    "    if len(word) > 1:\n",
    "        word = word[0].upper() + word[1:-1] + word[-1].upper()\n",
    "    elif len(word) == 1:\n",
    "        word = word.upper()\n",
    "    result.append(word)\n",
    "print(' '.join(result))"
